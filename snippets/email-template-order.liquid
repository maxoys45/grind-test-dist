                                                    <a href="https://{{ shop.domain }}/tools/emails/click/order-confirmation/1/logo/link?url={% capture oe_uri %}{{ shop.url }}{% endcapture %}{{ oe_uri | strip | url_encode }}" target="_blank" style="text-decoration: none !important; text-underline: none;">
                                                        <img src="https://d1oo2t5460ftwl.cloudfront.net/api/file/djfWJmSlRj3Hl2iyjtsg/convert?fit=max&w=416" class="logo " width="208" border="0" style="width: 208px; height: auto !important; display: block; text-align: center; Margin: auto;">
                                                    </a>
                                                    <!-- Logo : END -->
                                                </th>
                                            </tr>
                                        </table>
                                    </td>
