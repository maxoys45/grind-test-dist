            table.button {
                width: 100% !important;
            }
            th.order-table-title {
                text-align: center !important;
            }
            th.column_text_after_button {
                padding-right: 22px !important;
                padding-left: 22px !important;
            }
            .column_products th {
                vertical-align: top !important;
            }
        }
